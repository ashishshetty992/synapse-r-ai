{
  "title": "Product: top category by revenue (30d)",
  "entity": "product",
  "intent": {
    "ask": "top_k",
    "metric": {
      "op": "sum",
      "target": "product.price"
    },
    "target": "product.category"
  },
  "iql": {
    "ask": "top_k",
    "metric": {
      "op": "sum",
      "target": "product.price"
    },
    "target": "product.category",
    "timeWindow": {
      "type": "relative",
      "period": "last_30d"
    },
    "groupBy": [
      "product.category"
    ],
    "orderBy": [
      {
        "expr": "metric",
        "dir": "desc"
      }
    ],
    "limit": 10
  }
}